[global]
ioengine=libaio
direct=1
buffered=0
time_based=1
runtime=60
size=8G
group_reporting=1
iodepth=4
bs=8k
rw=write
[Test_sda]
filename=/dev/sda
[Test_sdb]
filename=/dev/sdb
